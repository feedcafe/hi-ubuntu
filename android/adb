adb shell input text helloworld	# type helloworld in TextView

adb shell input keyevent 3	# KEYCODE_HOME
adb shell input keyevent 4	# KEYCODE_BACK
adb shell input keyevent 5	# KEYCODE_CALL
adb shell input keyevent 6	# KEYCODE_ENDCALL
adb shell input keyevent 7	# KEYCODE_0
adb shell input keyevent 8	# KEYCODE_1
adb shell input keyevent 9	# KEYCODE_2
adb shell input keyevent 10	# KEYCODE_3
adb shell input keyevent 11	# KEYCODE_4
adb shell input keyevent 12	# KEYCODE_5
adb shell input keyevent 13	# KEYCODE_6
adb shell input keyevent 14	# KEYCODE_7
adb shell input keyevent 15	# KEYCODE_8
adb shell input keyevent 16	# KEYCODE_9
adb shell input keyevent 17	# KEYCODE_STAR
adb shell input keyevent 18	# KEYCODE_POUND
adb shell input keyevent 19	# KEYCODE_DPAD_UP
adb shell input keyevent 20	# KEYCODE_DPAD_DOWN
adb shell input keyevent 21	# KEYCODE_DPAD_LEFT
adb shell input keyevent 22	# KEYCODE_DPAD_RIGHT
adb shell input keyevent 23	# KEYCODE_DPAD_CENTER
adb shell input keyevent 24	# KEYCODE_VOLUME_UP
adb shell input keyevent 25	# KEYCODE_VOLUME_DOWN
adb shell input keyevent 26	# KEYCODE_POWER

adb shell input keyevent 66	# KEYCODE_ENTER
adb shell input keyevent 82	# KEYCODE_MENU
